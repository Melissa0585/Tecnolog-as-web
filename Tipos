type char = {
  poder: string,
  estatura: number,
};


// Multiples tipos
// cree dos tipos, uno para charles y otro para apocalipsis
let charles = {
  poder:"psiquico",
  estatura: 1.78
};


type apoca = {
 lider: boolean,
  miembros: string[];
 
};

let apocalipsis = {
  lider:true,
  miembros: ["Magneto","Tormenta","Psylocke","Angel"]
}

